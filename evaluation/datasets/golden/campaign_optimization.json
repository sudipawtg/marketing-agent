          {
            "type": "audience_targeting",
            "priority": "high",
            "action": "Refine audience segments based on conversion data"
          },
          {
            "type": "creative_optimization",
            "priority": "medium",
            "action": "Test new ad creatives with A/B testing"
          }
        ],
        "reasoning": [
          "Conversion rate is 60% below benchmark",
          "Cost per conversion is 2x the target",
          "Trend analysis shows declining performance"
        ],
        "confidence": 0.85,
        "keywords": [
          "budget",
