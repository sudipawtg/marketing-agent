        "signals": [
          {
            "type": "performance",
            "metric": "conversion_rate",
            "value": 0.01,
            "benchmark": 0.025,
            "trend": "declining",
            "severity": "high"
          },
          {
            "type": "cost",
            "metric": "cost_per_conversion",
            "value": 300,
            "benchmark": 150,
            "trend": "increasing",
            "severity": "high"
          }
        ],
        "context": {
          "industry": "retail",
          "season": "summer",
          "competitors": ["CompX", "CompY"],
          "target_audience": "millennials_gen_z"
